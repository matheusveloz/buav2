{
  "functions": {
    "app/api/audio/upload/route.ts": {
      "maxDuration": 300,
      "memory": 1024
    },
    "app/api/**/*.ts": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "crons": [
    {
      "path": "/api/cron/process-images",
      "schedule": "*/5 * * * *"
    }
  ],
  "regions": ["iad1"],
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs"
}

